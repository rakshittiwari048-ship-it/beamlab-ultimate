# ============================================
# BeamLab Ultimate - Python Backend
# Environment Configuration
# ============================================
# Domain: beamlabultimate.tech
# Copy this file to .env and fill in your values
# ============================================

# Server Configuration
NODE_ENV=development
PORT=8001

# Google AI API Key (REQUIRED for Gemini AI generation)
# Get from: https://aistudio.google.com/app/apikey
# 1. Go to Google AI Studio
# 2. Create a new project or select existing
# 3. Generate API Key
# 4. Paste below (never commit real keys!)
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Structural Analysis Configuration
USE_MOCK_AI=True
# Set to False in production to use real AI
# True = Uses hardcoded mock responses (no API key needed)
# False = Uses real Gemini AI (requires valid API key)

ANALYSIS_TIMEOUT=30
# Timeout in seconds for AI generation requests

# Logging Configuration
LOG_LEVEL=DEBUG
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL

# CORS Configuration (for production)
ALLOWED_ORIGINS=https://beamlabultimate.tech,https://www.beamlabultimate.tech
